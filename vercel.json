{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
    ],
    "public": true,
    "functions": {
        "api/download.js": {
            "runtime": "nodejs14.x",
            "memory": 1024
        }
    }
}